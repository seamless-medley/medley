{
  "name": "@seamless-medley/radio",
  "version": "0.1.0",
  "description": "Play your music seamlessly like a professional radio station via your speaker, Discord, etc",
  "author": {
    "name": "Wittawas Nakkasem",
    "email": "vittee@hotmail.com"
  },
  "keywords": [
    "radio",
    "discord",
    "medley",
    "automation"
  ],
  "license": "AGPL-3.0",
  "main": "src/server/main.ts",
  "scripts": {
    "clean": "rimraf .turbo dist combine",
    "rebuild-opus-electron": "tsx scripts/rebuild-opus-electron",
    "dev:react": "vite --port 9586 --host",
    "dev:server-electron": "cross-env DEBUG=1 ELECTRON_RUN_AS_NODE=1 electron --require ts-node/register -- src/server/main.ts",
    "dev:server": "cross-env MEDLEY_DEV_NO_TRACE=1 DEBUG=1 ts-node src/server/main.ts",
    "dev:bot": "cross-env DEBUG=1 ts-node src/discord/bot_main.ts",
    "dev:bot-electron": "cross-env DEBUG=1 ELECTRON_RUN_AS_NODE=1 electron --require ts-node/register -- src/discord/bot_main.ts",
    "debug:bot": "cross-env DEBUG=1 tsx --inspect --trace-warnings src/discord/bot_main.ts",
    "combine-discord": "tsx scripts/combine-discord.ts",
    "combine-discord:docker": "docker build -f ./docker/discord/Dockerfile ../..",
    "build": "tsc --build",
    "vb": "vite build"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^11.1.0",
    "@discordjs/opus": "0.9.0",
    "@discordjs/rest": "2.2.0",
