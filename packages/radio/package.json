{
  "name": "@seamless-medley/radio",
  "version": "0.1.0",
  "description": "Play your music seamlessly like a professional radio station via your speaker, Discord, etc",
  "author": {
    "name": "Wittawas Nakkasem",
    "email": "vittee@hotmail.com"
  },
  "keywords": [
    "radio",
    "discord",
    "medley",
    "automation"
  ],
  "license": "AGPL-3.0",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf .turbo dist",
    "copy-native": "copyfiles --up 4 ../node-medley/build/Release/medley.node build/Release/ || true",
    "dev:react": "vite --port 9586",
    "dev:server": "cross-env MEDLEY_DEV=1 DEBUG=1 tsx src/server/main.ts",
    "poc:bot": "cross-env MEDLEY_DEV=1 DEBUG=1 tsx src/discord/bot_main.ts",
    "debug:bot": "cross-env MEDLEY_DEV=1 DEBUG=1 tsx --inspect --trace-warnings src/discord/bot_main.ts",
    "poc:icy": "cross-env MEDLEY_DEV=1 tsx watch src/poc-icy.ts",
    "poc:shout": "cross-env MEDLEY_DEV=1 tsx watch src/poc-shout.ts",
    "sample": "cross-env MEDLEY_DEV=1 DEBUG=1 ts-node src/discord/sample_main.ts",
    "build": "tsc --build",
    "vb": "vite build"
  },
  "dependencies": {
    "@discordjs/opus": "0.9.0",
    "@discordjs/rest": "1.5.0",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@mantine/core": "5.9.6",
    "@mantine/hooks": "5.9.6",
    "@sapphire/async-queue": "^1.5.0",
    "@sapphire/snowflake": "^3.4.0",
    "@seamless-medley/core": "workspace:../core",
    "@seamless-medley/utils": "workspace:../utils",
    "@types/ffmpeg-static": "^3.0.1",
    "@types/normalize-path": "^3.0.0",
    "axios": "1.2.2",
    "discord-api-types": "0.37.31",
    "discord.js": "^14.8.0",
    "eventemitter3": "^5.0.0",
    "express": "4.18.2",
    "ffmpeg-static": "5.1.0",
    "framer-motion": "8.0.2",
    "jotai": "2.0.0",
    "jotai-valtio": "^0.2.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.34",
    "minimatch": "6.1.6",
    "mongodb": "4.13.0",
    "normalize-path": "^3.0.0",
    "notepack.io": "^3.0.1",
    "opus-decoder": "^0.6.2",
    "opusscript": "^0.0.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.5.2",
    "socket.io-client": "^4.5.2",
    "socket.io-msgpack-parser": "^3.0.2",
    "sodium-native": "^4.0.1",
    "tiny-typed-emitter": "^2.1.0",
    "tslog": "4.7.1",
    "typed-emitter": "2.1.0",
    "valtio": "^1.9.0",
    "workerpool": "6.3.1",
    "ws": "^8.12.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-decorators": "^7.12.13",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@types/audioworklet": "^0.0.36",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.176",
    "@types/mime-types": "^2.1.1",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@types/sodium-native": "^2.3.5",
    "@types/workerpool": "6.1.0",
    "@types/ws": "^8.5.4",
    "@vitejs/plugin-react": "3.0.0",
    "vite": "4.0.3"
  }
}
