    "readable-stream": "^4.4.2",
    "reflect-metadata": "^0.2.1",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "socket.io-msgpack-parser": "^3.0.2",
    "sodium-native": "^4.0.4",
