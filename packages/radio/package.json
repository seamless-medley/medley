    "@tanstack/router": "0.0.1-beta.86",
    "axios": "1.6.0",
    "cfonts": "^3.2.0",
    "classnames": "^2.3.2",
    "color.js": "^1.2.0",
    "commander": "^11.1.0",
    "discord-api-types": "0.37.67",
    "discord.js": "^14.14.1",
    "eventemitter3": "^5.0.1",
    "express": "4.18.2",
    "ffmpeg-static": "5.2.0",
    "framer-motion": "10.16.4",
    "gradient-string": "^2.0.2",
    "jotai": "2.6.0",
    "jotai-valtio": "^0.4.0",
    "lodash": "^4.17.21",
    "mediasoup": "^3.13.10",
