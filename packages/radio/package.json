{
  "name": "@seamless-medley/radio",
  "version": "0.1.0",
  "description": "Play your music seamlessly like a professional radio station, to your speaker, Discord, etc",
  "author": {
    "name": "Wittawas Nakkasem",
    "email": "vittee@hotmail.com"
  },
  "keywords": [
    "radio",
    "discord",
    "medley",
    "automation"
  ],
  "license": "AGPL-3.0",
  "main": "dist/index.js",
  "scripts": {
    "copy-native": "copyfiles --up 4 ../node-medley/build/Release/medley.node build/Release/ || true",
    "dev": "npm-run-all copy-native --parallel dev:*",
    "dev:react": "vite",
    "dev:server": "cross-env MEDLEY_DEMO=1 DEBUG=1 tsx watch src/server/main.ts",
    "poc:bot": "cross-env MEDLEY_DEMO=1 DEBUG=1 ts-node src/discord/bot_main.ts",
    "debug:bot": "cross-env MEDLEY_DEMO=1 DEBUG=1 tsx -- --inspect src/discord/bot_main.ts",
    "poc:icy": "cross-env MEDLEY_DEMO=1 tsx watch src/poc-icy.ts",
    "poc:shout": "cross-env MEDLEY_DEMO=1 tsx watch src/poc-shout.ts",
    "sample": "cross-env MEDLEY_DEMO=1 DEBUG=1 tsx src/discord/sample_main.ts",
    "build": "tsc --build",
    "vb": "vite build"
  },
  "dependencies": {
    "@discordjs/opus": "0.9.0",
    "@discordjs/rest": "1.5.0",
    "@discordjs/voice": "0.14.0",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mantine/core": "^5.0.2",
    "@mantine/hooks": "^5.0.2",
    "@seamless-medley/core": "workspace:../core",
    "@types/ffmpeg-static": "^3.0.1",
    "@types/normalize-path": "^3.0.0",
    "axios": "1.2.1",
    "discord-api-types": "0.37.25",
    "discord.js": "^14.7.1",
    "express": "^4.17.2",
    "framer-motion": "5",
    "ffmpeg-static": "5.1.0",
    "jotai": "^1.7.6",
    "libsodium-wrappers": "^0.7.9",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.34",
    "minimatch": "5.1.2",
    "mongodb": "4.13.0",
    "normalize-path": "^3.0.0",
    "opusscript": "^0.0.8",
    "socket.io": "^4.5.2",
    "socket.io-client": "^4.5.2",
    "tslog": "4.7.1",
    "workerpool": "6.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.176",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^16.11.17",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/workerpool": "6.1.0",
    "@vitejs/plugin-react": "^1.1.3",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsx": "3.12.1",
    "type-fest": "3.5.0",
    "typed-emitter": "2.1.0",
    "typescript": "^4.5.4",
    "vite": "^2.7.10"
  }
}
