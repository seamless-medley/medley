{
  "name": "medley-app",
  "version": "0.0.0",
  "description": "",
  "main": "bundle/main.js",
  "author": "",
  "license": "AGPL-3.0",
  "scripts": {
    "start": "electron .",
    "build": "webpack --config webpack.main.js",
    "pack": "electron-builder"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.13",
    "@medley/core": "file:../core",
    "@medley/medley": "file:../node-medley"
  },
  "browserslist": "electron 11.2",
  "build": {
    "appId": "com.github.vittee.medley",
    "files": [
      "!**/*",
      "./bundle/**/*"
    ],
    "extraFiles": [
      "./build/Release/medley.node"
    ],
    "asar": false,
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-decorators": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.16",
    "@types/node": "^14.14.27",
    "babel-loader": "^8.2.2",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "electron": "^11.2.3",
    "electron-builder": "^22.9.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0"
  }
}
